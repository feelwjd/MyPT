        <activity android:name=".CheckBody"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".Routine"></activity>
        <activity android:name=".SignDeleteActivity"></activity>
        <activity android:name=".DbTest"></activity>
        <activity android:name=".My_TestActivity"></activity>
        <activity android:name=".Watch"></activity>
