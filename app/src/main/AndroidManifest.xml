<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mypt">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyPT"
        android:usesCleartextTraffic="true"
        >
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Watch"></activity>
        <activity android:name=".TestActivity"></activity>
        <activity android:name=".Calender"></activity>
        <activity android:name=".DbTest"></activity>
        <activity android:name=".BodyPicture"></activity>
        <activity android:name=".CheckBody"></activity>
        <activity android:name=".Routine"></activity>
        <activity android:name=".Community_UpLoad"></activity>

        <activity android:name=".CAL_TestActivity"></activity>

    </application>

</manifest>